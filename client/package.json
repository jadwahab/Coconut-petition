{
  "name": "MA-SDC-Client",
  "version": "1.0.0",
  "description": "Simple Client for MA-SDC",
  "main": "index.js",
  "scripts": {
    "analyze": "cross-env ANALYZE_ENV=true yarn build",
    "analyze:production": "cross-env ANALYZE_ENV=true yarn build:production",
    "build": "webpack",
    "build:production": "cross-env NODE_ENV=production webpack",
    "start": "webpack-dev-server",
    "run_test": "set NODE_ENV=test&&npm run test",
    "test": "./node_modules/.bin/mocha --require ./test/setup.js --compilers js:babel-core/register --recursive --no-timeouts",
    "benchmark": "cross-env NODE_ENV=test ./node_modules/.bin/mocha benchmark/*.js --require ./benchmark/setup.js --compilers js:babel-core/register --recursive --no-timeouts"
  },
  "author": "Jedrzej Stuczynski",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "cross-env": "^5.1.3",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "script-loader": "^0.7.2",
    "semantic-ui-react": "^0.78.2",
    "webpack": "^3.11.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-airbnb": "^2.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jsdom": "^11.11.0",
    "mocha": "^4.1.0",
    "sinon": "^4.3.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
